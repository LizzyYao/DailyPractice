year1=input("Please enter year you would like to check: ")
year=int(year1)


if year%4==0:
    isLeapYear=not(year%100==0 and year%400!=0)
else:
    isLeapYear = False


def isLeapYearPrint(isLeapYear=True):
    print ("This year {} is leap year.".format(year1))
def notLeapYearPrint(isLeapYear=False):
    print ("This year {} is not leap year.".format(year1))
    


isLeapYearPrint(isLeapYear=True)

notLeapYearPrint(isLeapYear=False)
